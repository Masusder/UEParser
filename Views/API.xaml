<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="UEParser.Views.API"
             xmlns:vm="clr-namespace:UEParser.ViewModels"
             x:DataType="vm:APIModel">
  <StackPanel Margin="20" VerticalAlignment="Center" HorizontalAlignment="Center">
    <TextBlock HorizontalAlignment="Center" Text="{Binding Version}" Margin="10" VerticalAlignment="Center" />
    <Button Content="Fetch Data" HorizontalAlignment="Center" VerticalAlignment="Center" />
  </StackPanel>
</UserControl>