<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sty="using:FluentAvalonia.Styling"
             x:Class="UEParser.App"
             RequestedThemeVariant="Dark">
  <Application.Styles>
    <sty:FluentAvaloniaTheme />
  </Application.Styles>

  <Application.Resources>
    <ResourceDictionary>
      <Color x:Key="SystemAccentColor">#1E90FF</Color>
      <Color x:Key="SystemAccentColorLight1">#63B8FF</Color>
      <Color x:Key="SystemAccentColorLight2">#1E90FF</Color> <!--#87CEFA -->
      <Color x:Key="SystemAccentColorLight3">#B0E2FF</Color>
      <Color x:Key="SystemAccentColorDark1">#1C86EE</Color>
      <Color x:Key="SystemAccentColorDark2">#1874CD</Color>
      <Color x:Key="SystemAccentColorDark3">#104E8B</Color>
      <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="avares://LoadingIndicators.Avalonia/LoadingIndicators.axaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
